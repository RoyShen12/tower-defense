{
  "compilerOptions": {
    "target": "esnext",
    "checkJs": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "suppressExcessPropertyErrors": true,
    "suppressImplicitAnyIndexErrors": true 
  },
  "include": [
    "*.js"
  ],
  "typeAcquisition": {
    "include": [
      "lodash"
    ]
  }
}